    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "antd": "^4.3.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.14.3",
    "apollo-server-express": "^2.14.4",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "jsonwebtoken": "^8.5.1",
    "node-tradfri-client": "^2.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-scripts": "3.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "watch:frontend": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "authenticate": "ts-node --project ./server/tsconfig.json ./server/tradfri/authenticate.ts",
    "server": "ts-node --project ./server/tsconfig.json ./server/index.ts",
    "watch:backend": "nodemon --ext ts,json --watch server --exec ts-node --project ./server/tsconfig.json ./server/index.ts",
    "generate": "graphql-codegen -r dotenv/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.2",
    "@graphql-codegen/near-operation-file-preset": "^1.15.2",
    "@graphql-codegen/typescript": "^1.15.2",
    "@graphql-codegen/typescript-operations": "^1.15.2",
    "@graphql-codegen/typescript-resolvers": "^1.15.2",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.12.44",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  }
